    "typescript": "^4.6.2"
  },
  "keywords": [
    "react",
    "react-native",
    "react-navigation",
    "react-native-starter"
  ],
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "MIT"
}
